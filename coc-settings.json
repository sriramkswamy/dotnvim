{
	"coc.source.vimtex.enable": true,
    "coc.source.tag.enable": true,
    "coc.source.word.enable": true,
    "coc.source.omni.enable": true,
    "coc.source.file.enable": true,
    "coc.source.syntax.enable": false,
    "coc.source.ultisnips.enable": true,

    "coc.source.tag.priority": 99,
    "coc.source.file.priority": 10,
    "coc.source.word.priority": 4,
    "coc.source.omni.priority": 2,
    "coc.source.vimtex.priority": 1,
    "coc.source.ultisnips.priority": 3,

    "python.analysis.openFilesOnly": true,
    "python.jediEnabled": false,
    "python.pythonPath": "/usr/bin/python",
    "python.venvFolders": [
        "envs",
        ".pyenv",
        ".direnv",
        "~/.virtualenvs"
    ],

    "diagnostic.displayByAle": true,
    "diagnostic.errorSign": ">>",
    "diagnostic.warningSign": "!!"

}
