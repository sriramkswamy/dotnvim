	"MPLS": {
		"command": "dotnet",
		"args": ["exec", "/Users/esn89/python-language-server/output/bin/Debug/Microsoft.Python.LanguageServer.dll"],
		"filetypes": ["python"],
		"initializationOptions": {
			"interpreter": {
				"properties": {
					"InterpreterPath": "/usr/local/bin/python",
					"UseDefaultDatabase": true,
					"Version": "2.7.15"
				}
			}
		}
	}
